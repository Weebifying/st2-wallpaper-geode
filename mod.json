{
	"geode": "2.0.0-beta.23",
	"gd": {
		"win": "2.204",
		"android": "2.205"
	},
	"version": "v1.0.0",
	"id": "weebify.st2_wallpaper",
	"name": "ST2 Wallpaper",
	"developers": [
		"Weebify",
		"Spectra"
	],	
	"description": "Custom Menu Wallpaper for Sunix Thingy 2",
	"resources": {
		"files": [
			"res/*.png",
			"res/*.plist"
		]
	},
	"dependencies": [
		{
			"id": "geode.node-ids",
			"version": ">=1.8.0",
			"importance": "required"
		}
	],
	"settings": {
		"only_main": {
			"name": "Wallpaper in only Main Menu",
			"type": "bool",
			"default": false
		},
		"particles": {
			"name": "Toggle particles",
			"type": "bool",
			"default": true
		}
	}
}